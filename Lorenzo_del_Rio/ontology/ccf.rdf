    <!-- http://data.creditclassification.biz/ontology/ccf#credit_evaluation -->

    <owl:Class rdf:about="http://data.creditclassification.biz/ontology/ccf#credit_evaluation">
        <rdfs:subClassOf rdf:resource="https://schema.org/LoanOrCredit"/>
        <rdfs:subClassOf>
            <owl:Restriction>
            <owl:onProperty rdf:resource="http://data.creditclassification.biz/ontology/ccf#has_credit_requestor"/>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>           
        <rdfs:comment>Loan info enriched with classification of the risk</rdfs:comment>
        <AnnotationVocabulary1:usageNote>URIs for credit_evaluation individuals are of the form : http://data.creditclassification.biz/resource/ccf/credit_evaluation/&lt;credit_evaluation_id&gt; . &lt;credit_evaluation_id&gt;  is E&lt;integer&gt;</AnnotationVocabulary1:usageNote>
    </owl:Class>
    


    <!-- http://data.creditclassification.biz/ontology/ccf#credit_requestor -->

    <owl:Class rdf:about="http://data.creditclassification.biz/ontology/ccf#credit_requestor">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/Party"/>
        <rdfs:subClassOf>
            <owl:Restriction>
            <owl:onProperty rdf:resource="http://data.creditclassification.biz/ontology/ccf#has_credit_evaluation"/>
            <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>        
        <rdfs:comment>Info of a credit requestor in the context of the evaluation of the risk of a requested loan.</rdfs:comment>
        <AnnotationVocabulary1:usageNote>URIs for credit_requestor individuals are of the form : http://data.creditclassification.biz/resource/ccf/credit_requestor/&lt;credit_requestor_id&gt; . &lt;credit_requestor_id&gt;  is R&lt;integer&gt;</AnnotationVocabulary1:usageNote>
    </owl:Class>
    


