@base <http://data.creditclassification.biz/ontology/ccf#> .

<http://data.creditclassification.biz/ontology/ccf> rdf:type owl:Ontology ;
                                                     owl:imports <http://www.w3.org/2004/02/skos/core> ,
                                                                 schema: ,
                                                                 <https://spec.edmcouncil.org/fibo/ontology/master/2024Q4/FND/Parties/Parties/> ;
                                                     cc:license <https://creativecommons.org/licenses/by-nc-sa/4.0/> ;
                                                     dcterms:creator "Lorenzo del Río" ;
                                                     dcterms:description "An ontology for modeling a credit classification dataset." ;
                                                     dcterms:license <https://creativecommons.org/licenses/by-nc-sa/4.0/> ;
                                                     dcterms:title "Credit Classification Ontology" ;
                                                     foaf:homepage <http://data.creditclassification.biz/ontology/ccf> .

#################################################################
#    Annotation properties
#################################################################

###  http://creativecommons.org/ns#license
cc:license rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2001/XMLSchema#pattern
xsd:pattern rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/homepage
foaf:homepage rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://data.creditclassification.biz/ontology/ccf#employment_status_enum
:employment_status_enum rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://data.creditclassification.biz/ontology/ccf#age_when_risk_evaluated
:age_when_risk_evaluated rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf schema:duration ;
                         rdfs:domain :credit_evaluation ;
                         rdfs:range [ rdf:type owl:Class ;
                                      owl:unionOf ( schema:Duration
                                                    schema:QuantitativeValue
                                                    schema:Text
                                                  )
                                    ] ;
                         rdfs:comment "Age of the credit_requestor at the time the risk was evaluated. In years, in ISO 8601 format" .


###  http://data.creditclassification.biz/ontology/ccf#has_class
:has_class rdf:type owl:ObjectProperty ;
           rdfs:domain :credit_evaluation ;
           rdfs:range :class_code ;
           rdfs:label "has credit risk class"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_credit_evaluation
:has_credit_evaluation rdf:type owl:ObjectProperty ;
                       owl:inverseOf :has_credit_requestor ;
                       rdfs:domain :credit_requestor ;
                       rdfs:range :credit_evaluation ;
                       rdfs:label "Credit evaluation of a credit requestor"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_credit_history
:has_credit_history rdf:type owl:ObjectProperty ;
                    rdfs:domain :credit_requestor ;
                    rdfs:range :credit_history_code ;
                    rdfs:label "has credit history"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_credit_requestor
:has_credit_requestor rdf:type owl:ObjectProperty ;
                      rdfs:domain :credit_evaluation ;
                      rdfs:range :credit_requestor ;
                      rdfs:label "Credit requestor of a credit evaluation"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_foreign_worker
:has_foreign_worker rdf:type owl:ObjectProperty ;
                    rdfs:domain :credit_requestor ;
                    rdfs:range :foreign_worker_code ;
                    rdfs:label "has foreign worker status"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_housing
:has_housing rdf:type owl:ObjectProperty ;
             rdfs:domain :credit_requestor ;
             rdfs:range :housing_code ;
             rdfs:label "has housing"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_job
:has_job rdf:type owl:ObjectProperty ;
         rdfs:domain :credit_requestor ;
         rdfs:range :job_code ;
         rdfs:label "has job"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_other_debtors_guarantors
:has_other_debtors_guarantors rdf:type owl:ObjectProperty ;
                              rdfs:domain :credit_requestor ;
                              rdfs:range :other_debtors_guarantors_code ;
                              rdfs:label "has other debtors or guarantors"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_other_installment_plans
:has_other_installment_plans rdf:type owl:ObjectProperty ;
                             rdfs:domain :credit_requestor ;
                             rdfs:range :other_installment_plans_code ;
                             rdfs:label "has other installment plans"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_personal_status_sex
:has_personal_status_sex rdf:type owl:ObjectProperty ;
                         rdfs:domain :credit_requestor ;
                         rdfs:range :personal_status_sex_code ;
                         rdfs:label "has personal status and sex"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_present_employment
:has_present_employment rdf:type owl:ObjectProperty ;
                        rdfs:domain :credit_requestor ;
                        rdfs:range :present_employment_code ;
                        rdfs:label "has present employment info"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_present_residence
:has_present_residence rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf schema:duration ;
                       rdfs:domain :credit_requestor ;
                       rdfs:range schema:Text ;
                       rdfs:comment "Time the credit requestor has been in its current residence. In years, in ISO 8601 format" .


###  http://data.creditclassification.biz/ontology/ccf#has_property
:has_property rdf:type owl:ObjectProperty ;
              rdfs:domain :credit_requestor ;
              rdfs:range :property_code ;
              rdfs:label "has property info"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_purpose
:has_purpose rdf:type owl:ObjectProperty ;
             rdfs:domain :credit_evaluation ;
             rdfs:range :purpose_code ;
             rdfs:label "has purpose"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_savings_account
:has_savings_account rdf:type owl:ObjectProperty ;
                     rdfs:domain :credit_requestor ;
                     rdfs:range :savings_account_code ;
                     rdfs:label "has savings account info"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_sex
:has_sex rdf:type owl:ObjectProperty ;
         rdfs:domain :credit_requestor ;
         rdfs:range :Sex ;
         rdfs:comment "The biological sex of the credit requestor."@en ;
         rdfs:label "has sex"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_status_checking_account
:has_status_checking_account rdf:type owl:ObjectProperty ;
                             rdfs:domain :credit_requestor ;
                             rdfs:range :status_checking_account_code ;
                             rdfs:label "has checking account status"@en .


###  http://data.creditclassification.biz/ontology/ccf#has_telephone
:has_telephone rdf:type owl:ObjectProperty ;
               rdfs:domain :credit_requestor ;
               rdfs:range :telephone_code ;
               rdfs:label "has telephone"@en .


#################################################################
#    Data properties
#################################################################

###  http://data.creditclassification.biz/ontology/ccf#has_employment_status
:has_employment_status rdf:type owl:DatatypeProperty ;
                       rdfs:domain :credit_requestor ;
                       rdfs:range :employment_status_enum ;
                       rdfs:comment """
    The employment status as a string: 'employed' or 'unemployed'.
    Refer to DBpedia for more information.
  """@en ;
                       rdfs:label "employment status of the credit requestor"@en ;
                       rdfs:seeAlso <http://dbpedia.org/resource/Employment> ,
                                    <http://dbpedia.org/resource/Unemployment> .


###  http://data.creditclassification.biz/ontology/ccf#has_installment_rate
:has_installment_rate rdf:type owl:DatatypeProperty ;
                      rdfs:domain :credit_evaluation ;
                      rdfs:range xsd:decimal ;
                      rdfs:comment "Installment rate in percentage of disposable income" ;
                      rdfs:label "Installment rate"@en .


###  http://data.creditclassification.biz/ontology/ccf#number_of_dependents
:number_of_dependents rdf:type owl:DatatypeProperty ;
                      rdfs:domain :credit_requestor ;
                      rdfs:range xsd:integer ;
                      rdfs:comment "Number of persons that depend on the credit requestor" .


###  http://data.creditclassification.biz/ontology/ccf#number_of_existing_credits
:number_of_existing_credits rdf:type owl:DatatypeProperty ;
                            rdfs:domain :credit_requestor ;
                            rdfs:range xsd:integer ;
                            rdfs:comment "Number of other credits that the credit_requestor has" .


#################################################################
#    Classes
#################################################################

###  http://data.creditclassification.biz/ontology/ccf#Sex
:Sex rdf:type owl:Class ;
     owl:equivalentClass [ rdf:type owl:Class ;
                           owl:oneOf ( <http://dbpedia.org/resource/Female>
                                       <http://dbpedia.org/resource/Male>
                                     )
                         ] ;
     rdfs:comment """
        The class of biological sexes, limited to the individuals Male and Female from DBpedia.
        """@en ;
     rdfs:label "Sex"@en .


###  http://data.creditclassification.biz/ontology/ccf#class_code
:class_code rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( skos:Concept
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty skos:inScheme ;
                                                         owl:hasValue :class
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf <https://www.omg.org/spec/Commons/CodesAndCodeSets/CodeElement> ;
            rdfs:comment """
        Risk classification of the credit.
        """@en ;
            rdfs:label "Risk classification of the credit"@en .


###  http://data.creditclassification.biz/ontology/ccf#credit_evaluation
:credit_evaluation rdf:type owl:Class ;
                   rdfs:subClassOf schema:LoanOrCredit ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :has_credit_requestor ;
                                     owl:cardinality "1"^^xsd:nonNegativeInteger
                                   ] ;
                   rdfs:comment "Loan info enriched with classification of the risk" ;
                   AnnotationVocabulary1:usageNote "URIs for credit_evaluation individuals are of the form : http://data.creditclassification.biz/resource/ccf/credit_evaluation/<credit_evaluation_id> . <credit_evaluation_id>  is E<integer>" .


###  http://data.creditclassification.biz/ontology/ccf#credit_history_code
:credit_history_code rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( skos:Concept
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty skos:inScheme ;
                                                                  owl:hasValue :credit_history
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf <https://www.omg.org/spec/Commons/CodesAndCodeSets/CodeElement> ;
                     rdfs:comment """
        A classification code providing info about the credit history of the credit requestor.
        """@en ;
                     rdfs:label "Credit history Code"@en .


###  http://data.creditclassification.biz/ontology/ccf#credit_requestor
:credit_requestor rdf:type owl:Class ;
                  rdfs:subClassOf <https://www.omg.org/spec/Commons/PartiesAndSituations/Party> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :has_credit_evaluation ;
                                    owl:minCardinality "0"^^xsd:nonNegativeInteger
                                  ] ;
                  rdfs:comment "Info of a credit requestor in the context of the evaluation of the risk of a requested loan." ;
                  AnnotationVocabulary1:usageNote "URIs for credit_requestor individuals are of the form : http://data.creditclassification.biz/resource/ccf/credit_requestor/<credit_requestor_id> . <credit_requestor_id>  is R<integer>" .


###  http://data.creditclassification.biz/ontology/ccf#employment_status_enum
:employment_status_enum rdf:type owl:Class ;
                        owl:equivalentClass <http://org.semanticweb.owlapi/error#Error1> .


###  http://data.creditclassification.biz/ontology/ccf#foreign_worker_code
:foreign_worker_code rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( skos:Concept
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty skos:inScheme ;
                                                                  owl:hasValue :foreign_worker
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf <https://www.omg.org/spec/Commons/CodesAndCodeSets/CodeElement> ;
                     rdfs:comment """
        A classification code telling if the credit requestor is foreign worker or not.
        """@en ;
                     rdfs:label "Foreign worker Code"@en .


###  http://data.creditclassification.biz/ontology/ccf#housing_code
:housing_code rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( skos:Concept
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty skos:inScheme ;
                                                           owl:hasValue :housing
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf <https://www.omg.org/spec/Commons/CodesAndCodeSets/CodeElement> ;
              rdfs:comment """
        A classification code providing info about the housing of the credit requestor.
        """@en ;
              rdfs:label "Housing Code"@en .


###  http://data.creditclassification.biz/ontology/ccf#job_code
:job_code rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( skos:Concept
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty skos:inScheme ;
                                                       owl:hasValue :job
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf <https://www.omg.org/spec/Commons/CodesAndCodeSets/CodeElement> ;
          rdfs:comment """
        A classification code providing info about the job of the credit requestor.
        """@en ;
          rdfs:label "Job Code"@en .


###  http://data.creditclassification.biz/ontology/ccf#other_debtors_guarantors_code
:other_debtors_guarantors_code rdf:type owl:Class ;
                               owl:equivalentClass [ owl:intersectionOf ( skos:Concept
                                                                          [ rdf:type owl:Restriction ;
                                                                            owl:onProperty skos:inScheme ;
                                                                            owl:hasValue :other_debtors_guarantors
                                                                          ]
                                                                        ) ;
                                                     rdf:type owl:Class
                                                   ] ;
                               rdfs:subClassOf <https://www.omg.org/spec/Commons/CodesAndCodeSets/CodeElement> ;
                               rdfs:comment """
        A classification code indicating info about other debtors/guarantors.
        """@en ;
                               rdfs:label "Other debtors guarantors Code"@en .


###  http://data.creditclassification.biz/ontology/ccf#other_installment_plans_code
:other_installment_plans_code rdf:type owl:Class ;
                              owl:equivalentClass [ owl:intersectionOf ( skos:Concept
                                                                         [ rdf:type owl:Restriction ;
                                                                           owl:onProperty skos:inScheme ;
                                                                           owl:hasValue :other_installment_plans
                                                                         ]
                                                                       ) ;
                                                    rdf:type owl:Class
                                                  ] ;
                              rdfs:subClassOf <https://www.omg.org/spec/Commons/CodesAndCodeSets/CodeElement> ;
                              rdfs:comment """
        A classification code indicating if the credit requestor has other installment plans.
        """@en ;
                              rdfs:label "Other installment plans code"@en .


###  http://data.creditclassification.biz/ontology/ccf#personal_status_sex_code
:personal_status_sex_code rdf:type owl:Class ;
                          owl:equivalentClass [ owl:intersectionOf ( skos:Concept
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty skos:inScheme ;
                                                                       owl:hasValue :personal_status_sex
                                                                     ]
                                                                   ) ;
                                                rdf:type owl:Class
                                              ] ;
                          rdfs:subClassOf <https://www.omg.org/spec/Commons/CodesAndCodeSets/CodeElement> ;
                          rdfs:comment """
        A classfication code that mixes info about the marital status and sex of the credit requestor.
        """@en ;
                          rdfs:label "Personal status sex code"@en .


###  http://data.creditclassification.biz/ontology/ccf#present_employment_code
:present_employment_code rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( skos:Concept
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty skos:inScheme ;
                                                                      owl:hasValue :present_employment
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:subClassOf <https://www.omg.org/spec/Commons/CodesAndCodeSets/CodeElement> ;
                         rdfs:comment """
        A classification code providing info about the present employment status of the credit requestor.
        """@en ;
                         rdfs:label "Present employment Code"@en .


###  http://data.creditclassification.biz/ontology/ccf#property_code
:property_code rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( skos:Concept
